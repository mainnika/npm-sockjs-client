  "devDependencies": {
    "greenkeeper-postpublish": "^1.0.1",
    "sockjs-client": "^1.1.2",
    "ts-node": "^2.1.0",
    "tslint": "^4.5.1",
    "tslint-config-typings": "^0.3.1",
